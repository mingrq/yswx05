<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.chiye.yswx05.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_product"
        android:name="com.chiye.yswx05.ui.product.ProductFragment"
        android:label="@string/title_product"
        tools:layout="@layout/fragment_product" />

    <fragment
        android:id="@+id/navigation_contactus"
        android:name="com.chiye.yswx05.ui.contactus.ContactusFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_contactus" />
    <fragment
        android:id="@+id/navigation_location"
        android:name="com.chiye.yswx05.ui.location.LocationFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_location" />
</navigation>